{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": [
            "./tsconfig.json"
        ]
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": [
        "*.html",
        "*.scss"
    ],
    "rules": {
        "no-console": 1,
        "no-empty": 0,
        "no-irregular-whitespace": 0
    }
}